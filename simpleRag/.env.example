# Backend Configuration
SPRING_BOOT_URL=http://localhost:8085

# LLM Configuration (Groq)
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=meta-llama/llama-3.1-8b-instant

# RAGAS Evaluation Configuration
# Provider can be GROQ or OPENAI
LLM_PROVIDER_EVAL=GROQ

# If using GROQ for Evaluation
EVAL_LLM_MODEL=llama-3.3-70b-versatile
EVAL_LLM_BINDING_API_KEY=your_groq_api_key_here

# If using OPENAI for Evaluation (or Embeddings)
OPENAI_API_KEY=your_openai_api_key_here
# EVAL_LLM_MODEL=gpt-4o-mini
# EVAL_LLM_BINDING_API_KEY=your_openai_api_key_here

# Embeddings for Evaluation
# Provider can be OPENAI or OLLAMA
EVAL_EMBEDDING_PROVIDER=OLLAMA

# If using OLLAMA (Recommended for local setup without OpenAI keys)
EVAL_EMBEDDING_MODEL=embeddinggemma:latest

# If using OPENAI
# EVAL_EMBEDDING_PROVIDER=OPENAI
# EVAL_EMBEDDING_MODEL=text-embedding-3-small
# EVAL_EMBEDDING_BINDING_API_KEY=your_openai_api_key_here

# JWT Authentication
JWT_SECRET=your_jwt_secret_here
JWT_ALGORITHM=HS256
